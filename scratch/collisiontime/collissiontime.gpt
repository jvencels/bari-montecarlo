set term pngcairo enhanced font "CM Roman"
set output 'collisiontime.png'

set title 'Cross-section vs. energy'
set xlabel 'Energy, eV'
set ylabel 'Cross-section, 10^{20} m^2'
set y2label 'Collision time, ns'

set xrange [0:1000]
set yrange [0:5]
#set y2range [0:1]
set logscale y2
set ytics nomirror
set y2tics

plot      'colldata.dat' u 1:2 w p title 'Experimental data' axes x1y1 linecolor rgb 'black', \
    'collissiontime.out' using 1:2 title 'Cross section' axes x1y1 with lines linecolor rgb 'red', \
     'collissiontime.out' using 1:3 title 'Collision time' axes x1y2 with lines linecolor rgb 'blue'